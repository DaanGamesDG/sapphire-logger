{
	"name": "@daangamesdg/sapphire-logger",
	"version": "1.0.4",
	"description": "A personal logger I use for all my Sapphire Discord bot projects",
	"author": "DaanGamesDG <daan@daangamesdg.xyz>",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./register": {
			"import": "./register.mjs",
			"require": "./register.js"
		}
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.2",
		"@commitlint/config-angular": "^17.0.0",
		"@sapphire/eslint-config": "^4.3.6",
		"@sapphire/framework": "^2.5.0",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/ts-config": "^3.3.4",
		"@types/node": "^16.11.38",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.0",
		"nodemon": "^2.0.16",
		"typescript": "^4.7.3"
	},
	"scripts": {
		"start:dev": "npm run build && npm run start",
		"dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts",
		"start": "cross-env NODE_ENV=production node .",
		"lint": "eslint src",
		"prepare": "husky install",
		"build": "tsc -b src && yarn esm:register && yarn esm:default",
		"esm:register": "gen-esm-wrapper dist/register.js dist/register.mjs",
		"esm:default": "gen-esm-wrapper dist/index.js dist/index.mjs"
	},
	"packageManager": "yarn@3.2.1",
	"dependencies": {
		"colorette": "^2.0.17",
		"discord.js": "^13.7.0",
		"tslib": "^2.4.0"
	}
}
