{
	"name": "@daangamesdg/sapphire-logger",
	"version": "1.0.2",
	"description": "A personal logger I use for all my Sapphire Discord bot projects",
	"author": "DaanGamesDG <daan@daangamesdg.xyz>",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./register": {
			"import": "./register.mjs",
			"require": "./register.js"
		}
	},
	"devDependencies": {
		"@commitlint/cli": "^15.0.0",
		"@commitlint/config-angular": "^15.0.0",
		"@sapphire/eslint-config": "^4.0.8",
		"@sapphire/prettier-config": "^1.2.7",
		"@sapphire/ts-config": "^3.1.6",
		"@types/node": "^16.11.14",
		"cross-env": "^7.0.3",
		"eslint": "^8.5.0",
		"gen-esm-wrapper": "^1.1.3",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.3",
		"nodemon": "^2.0.15",
		"typescript": "^4.5.4",
		"@sapphire/framework": "^2.2.1"
	},
	"scripts": {
		"start:dev": "npm run build && npm run start",
		"dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts",
		"start": "cross-env NODE_ENV=production node .",
		"lint": "eslint src",
		"prepare": "husky install",
		"build": "tsc -b src && yarn esm:register && yarn esm:default",
		"esm:register": "gen-esm-wrapper dist/register.js dist/register.mjs",
		"esm:default": "gen-esm-wrapper dist/index.js dist/index.mjs"
	},
	"packageManager": "yarn@3.1.1",
	"dependencies": {
		"colorette": "^2.0.16",
		"discord.js": "^13.3.1",
		"tslib": "^2.3.1"
	}
}
